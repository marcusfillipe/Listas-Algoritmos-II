#include <stdio.h>

void consumo(float km, float litros) {
    float consumo = km / litros;

    printf("Consumo: %.2f km/l - ", consumo);

    if (consumo < 8)
        printf("Venda o carro!\n");
    else if (consumo <= 14)
        printf("Econômico!\n");
    else
        printf("Super econômico!\n");
}
